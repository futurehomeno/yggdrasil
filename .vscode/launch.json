  "version": "0.2.0",
  "configurations": [
    {
      "name": "yggdrasil linux",
      "request": "launch",
      "type": "dart",
      "program": "lib/app/main.dart",
      "args": ["-d", "linux"]
    }, 
    {
      "name": "yggdrasil android",
      "request": "launch",
      "type": "dart",
      "program": "lib/app/main.dart",
      "args": ["-d", "emulator-5554"]
    },
    {
      "name": "yggdrasil (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "yggdrasil (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
